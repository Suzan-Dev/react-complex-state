{
  "name": "react-complex-state",
  "version": "1.1.0",
  "description": "A custom hook that makes it easy when working with complex state in react.",
  "author": "Suzan-Dev",
  "license": "MIT",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/esm/index.d.ts",
  "scripts": {
    "dev": "tsc -w",
    "build": "pnpm build:esm && pnpm build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir lib/cjs",
    "release": "dotenv release-it --"
  },
  "devDependencies": {
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "dotenv-cli": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "release-it": "^15.4.2",
    "typescript": "^4.8.3"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "files": [
    "/lib"
  ],
  "keywords": [
    "react",
    "react-hooks",
    "state"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Suzan-Dev/react-complex-state.git"
  },
  "bugs": {
    "url": "https://github.com/Suzan-Dev/react-complex-state/issues"
  },
  "homepage": "https://github.com/Suzan-Dev/react-complex-state",
  "release-it": {
    "github": {
      "release": true
    }
  }
